.certification {
  margin: 10px;
  font-weight: 400;
  height: 505px;
  object-fit: contain;
  background-size: 100% auto;
}

.block-el {
  display: block;
}

.logo {
  padding-left: 25px;
  width: 75px;
  height: 75px;
}

.display-wrapper {
  padding: 0 96px 0 48px;
}

.title-wrapper {
  padding-top: 85px;
  font-size: 12px;
  font-weight: 300;
  color: white;
  line-height: 1.3;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.winner-wrapper {
  padding-top: 20px;
  font-size: 35px;
  font-weight: 900;
  line-height: 1;
  text-transform: uppercase;
  color: white;
}

.subject {
  padding-top: 18px;
  font-size: 18px;
  text-transform: uppercase;
  font-weight: 900;
}

.hash-code {
  font-size: 10px;
  line-height: 1.2;
}

.date-wrapper {
  font-size: 10px;
  padding-top: 20px;
  color: white;
}

.expired-date {
  text-align: right;
}
